{
  "name": "ttag-cli",
  "version": "1.4.2",
  "main": "index.js",
  "repository": "git@github.com:ttag-org/ttag-cli.git",
  "author": "",
  "license": "MIT",
  "scripts": {
    "preversion": "yarn run build && yarn test",
    "test": "mkdir -p dist && jest",
    "cli": "ts-node ./src/index.ts",
    "build": "tsc",
    "upgrade": "yarn upgrade-interactive",
    "pretty": "prettier --write \"./src/**/*.ts\" \"./tests/**/*.ts\"",
    "precommit": "lint-staged",
    "postinstall": "./scripts/install_completion.js",
    "readme": "npm run build && node ./scripts/doc.js"
  },
  "lint-staged": {
    "*.{ts,css,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "bin": {
    "ttag": "./bin/ttag"
  },
  "devDependencies": {
    "@types/babel__core": "7.0.1",
    "@types/babel__generator": "^7.0.0",
    "@types/babel__parser": "^7.0.0",
    "@types/babel__template": "^7.0.0",
    "@types/babel__traverse": "^7.0.0",
    "@types/chalk": "^2.2.0",
    "@types/jest": "21.1.6",
    "@types/koa": "^2.0.43",
    "@types/koa-router": "^7.0.27",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "8.0.53",
    "@types/node-fetch": "^1.6.7",
    "@types/opn": "^5.1.0",
    "@types/ora": "1.3.1",
    "@types/readline-sync": "^1.4.2",
    "@types/serialize-javascript": "^1.3.2",
    "@types/tmp": "^0.0.33",
    "@types/yargs": "8.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "lint-staged": "^5.0.0",
    "prettier": "1.8.2",
    "ts-jest": "21.2.2",
    "ts-node": "3.3.0",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/generator": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/template": "^7.0.0",
    "ansi": "^0.3.1",
    "babel-plugin-ttag": "1.4.1",
    "chalk": "^2.3.0",
    "cross-spawn": "^5.1.0",
    "gettext-parser": "^1.3.0",
    "hunspell-spellchecker": "^1.0.2",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-router": "^7.3.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.7.3",
    "opn": "^5.1.0",
    "ora": "1.3.0",
    "plural-forms": "^0.2.0",
    "readline-sync": "^1.4.7",
    "serialize-javascript": "^1.4.0",
    "supports-color": "^5.0.1",
    "tmp": "0.0.33",
    "walk": "2.3.9",
    "yargs": "10.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
